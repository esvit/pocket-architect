      relations: [
        { type: 'n:n', entity: 'Item', name: 'products', ref: 'CartItem' }
      ]
    },
    {
      name: 'CartItem', type: 'valueObject', parent: 'cart',
      attributes: [
        { name: 'quantity', type: 'number', required: true }
      ]
