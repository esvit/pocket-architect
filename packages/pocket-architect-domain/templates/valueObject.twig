{%- set interfaceName = 'I' + valueObject.name -%}
import { ValueObject } from 'pocket-architect-core';

export interface {{interfaceName}} {
  {%- for attribute in valueObject.attributes %}
  {{ attribute.name }}{% if not attribute.mandatory %}?{% endif%}: {{ attribute.type }};
  {%- endfor %}
}

export
class {{valueObject.name}} extends ValueObject<{{interfaceName}}> {
  public static create(props: {{interfaceName}}): {{valueObject.name}} {
    const entity = new {{valueObject.name}}(props);
    return entity;
  }
}
